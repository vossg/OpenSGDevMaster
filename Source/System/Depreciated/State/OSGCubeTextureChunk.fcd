<?xml version="1.0"?>

<FieldContainer
	name="CubeTextureChunk"
	parent="TextureChunk"
	library="System"
	pointerfieldtypes="both"
	structure="concrete"
	systemcomponent="true"
	parentsystemcomponent="true"
	decoratable="false"
>
	<Field
		name="posZImage"
		type="ImagePtr"
		cardinality="single"
		visibility="external"
		access="public"
