OSG_BEGIN_NAMESPACE


inline UInt32 GeoIntegralProperty::getValue(const UInt32 index) const
{
    return getValue<UInt32>(index);
}

//! State Chunk class access

inline
UInt32 GeoIntegralProperty::getStaticClassId(void)
{
    return getStaticClass()->getId();
}

inline
const StateChunkClass *GeoIntegralProperty::getStaticClass(void)
{
    return &_class;
}

