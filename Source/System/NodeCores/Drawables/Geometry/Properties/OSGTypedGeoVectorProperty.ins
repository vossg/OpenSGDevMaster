OSG_BEGIN_NAMESPACE

template <class GeoPropertyDesc> inline
void TypedGeoVectorProperty<GeoPropertyDesc>::classDescInserter(
    TypeObject &oType)
{
    FieldDescriptionBase *pDesc = NULL;

    typedef typename StoredFieldType::Description SFDesc;

#ifdef OSG_1_COMPAT
    typedef const StoredFieldType *(Self::*GetMFFieldF)(void) const;

    GetMFFieldF GetMFField = &Self::getFieldPtr;
#endif

    pDesc = new SFDesc(
        StoredFieldType::getClassType(),
        "values",
        "",
        OSG_RC_FIELD_DESC(Self::GeoPropData),
        false,
        Field::MFDefaultFlags,
        reinterpret_cast<FieldEditMethodSig>(&Self::editFieldPtr),
#ifdef OSG_1_COMPAT
        reinterpret_cast<FieldGetMethodSig >(GetMFField));
#else
        reinterpret_cast<FieldGetMethodSig >(&Self::getFieldPtr ));
#endif

    oType.addInitialDesc(pDesc);
}


template <class GeoPropertyDesc>
typename TypedGeoVectorProperty<GeoPropertyDesc>::TypeObject
    TypedGeoVectorProperty<GeoPropertyDesc>::_type(
        Desc     ::getTypeName (),
        Inherited::getClassname(),
        Desc     ::getGroupName(),
        0,
        (PrototypeCreateF) &Self::createEmpty,
        &Self::initMethod,
        (InitalInsertDescFunc) &Self::classDescInserter,
        false,
        0);

OSG_END_NAMESPACE
