  #ff["getFieldType"].call_policies  = return_internal_reference()
  #ff["the"].call_policies = return_value_policy(reference_existing_object)
  #ff["_fieldTypeM"].exclude()
  #finalize(ff)
   
  fd = osg["FieldDescriptionBase"]
  fd.include()
